dIn={loggedIn} logout={logout}/>}\n         <Routes>\n            <Route path=\"/about\" element={<About />} />\n            <Route path=\"/home\" element={<Cards characters={characters} onClose={onClose}/>} />\n            <Route path=\"/detail/:id\" element={<Detail />} />\n            <Route path='*' element={<Error />} />\n            <Route path='/' element={<Form login={login} />} />\n            {loggedIn ? null : logout}\n         </Routes>\n      </div>\n   );\n};\n\n\n\nexport default App;\n",[],"/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/Cards/Cards.jsx",[],[],"/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/Nav/Nav.jsx",[],[],"/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/About/About.jsx",[],[],"/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/Error/Error.jsx",["62"],[],"import React, { Component } from 'react'\n\nexport class Error extends Component {\n    constructor(props) {\n        super(props);\n    }\n  render() {\n    return (\n      <div>\n        <h2>Error 404</h2>\n        <h3>This is not the web page you are looking for</h3>\n      </div>\n    )\n  }\n}\n\nexport default Error","/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/Detail/Detail.jsx",[],[],"/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/SearchBar/SearchBar.jsx",[],[],[],"/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/Card/Card.jsx",[],[],"/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/Form/Form.jsx",[],[],"/Users/macbook/Desktop/RICK-MORTY/Integrador-RickandMorty/CLIENT/src/components/Form/Validations.js",[],[],{"ruleId":"63","severity":1,"message":"64","line":57,"column":7,"nodeType":"65","endLine":57,"endColumn":15,"suggestions":"66"},{"ruleId":"67","severity":1,"message":"68","line":4,"column":5,"nodeType":"69","messag